1)git config --list --show-origin
file:/home/tafzs/.gitconfig     user.name=fayaz-abdul
file:/home/tafzs/.gitconfig     user.email=fayaz.abdul@gmail.com
file:.git/config        core.repositoryformatversion=0
file:.git/config        core.filemode=true
file:.git/config        core.bare=false
file:.git/config        core.logallrefupdates=true
file:.git/config        remote.origin.url=https://github.com/jeevanempati/documentations.git
file:.git/config        remote.origin.fetch=+refs/heads/*:refs/remotes/origin/*
file:.git/config        branch.master.remote=origin
file:.git/config        branch.master.merge=refs/heads/master

2)git config --global user.name "jeevanempati"
